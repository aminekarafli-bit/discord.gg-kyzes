        AutoButtonColor=false,ZIndex=51})
    Rnd(nb,8)

    -- Tooltip at ZIndex 200 — always on top of EVERYTHING
    local tt = Frame(SB, C.BG3, S(100,28), S(56, yP+5), 200, 7)
    Bdr(tt, C.BLU, 1)
    Label(tt, def.label, 12, C.WHT, S(92,28), S(4,0), nil, Enum.TextXAlignment.Center, 201)
    tt.Visible = false

    nb.MouseEnter:Connect(function()
        tt.Visible = true
        TW(nb,0.1,{BackgroundTransparency=0, BackgroundColor3=C.BG3})
    end)
    nb.MouseLeave:Connect(function()
        tt.Visible = false
        TW(nb,0.1,{BackgroundTransparency=1})
    end)
    nb.MouseButton1Click:Connect(function()
        if SwitchTab then SwitchTab(i) end
    end)
    NavBtns[i] = nb
end

