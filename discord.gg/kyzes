local KL_ICON_B64 = table.concat(_IC)

-- Write KL icon PNG to workspace on first run
local function EnsureIcon()
    if not (isfile and writefile) then return end
    if isfile(ICON_FILE) then return end
    pcall(function()
        local lk = {}
        for i = 0, 25 do lk[string.char(65+i)] = i end
        for i = 0, 25 do lk[string.char(97+i)] = i+26 end
        for i = 0, 9  do lk[string.char(48+i)] = i+52 end
        lk["+"] = 62 lk["/"] = 63
        local res = {}
        local data = KL_ICON_B64:gsub("[^A-Za-z0-9+/=]","")
        for i = 1, #data, 4 do
            local c1 = lk[data:sub(i,i)] or 0
            local c2 = lk[data:sub(i+1,i+1)] or 0
            local c3 = lk[data:sub(i+2,i+2)]
            local c4 = lk[data:sub(i+3,i+3)]
            local n  = c1*262144 + c2*4096
            res[#res+1] = string.char(math.floor(n/65536)%256)
            if c3 then n=n+c3*64  res[#res+1]=string.char(math.floor(n/256)%256) end
            if c4 then n=n+c4     res[#res+1]=string.char(n%256) end
        end
        writefile(ICON_FILE, table.concat(res))
    end)
end
pcall(EnsureIcon)

-- HWID
local function GetHWID()
    if isfile and isfile(HWID_FILE) then
        local v = readfile(HWID_FILE)
        if v and #v > 10 then return v end
    end
    local c = "ABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789"
    local s = ""
    for i = 1,32 do local r=math.random(1,#c) s=s..c:sub(r,r) end
    local h = s:sub(1,8).."-"..s:sub(9,12).."-"..s:sub(13,16).."-"..s:sub(17,20).."-"..s:sub(21,32)
    if writefile then pcall(function() writefile(HWID_FILE, h) end) end
    return h
end
local HWID = GetHWID()

