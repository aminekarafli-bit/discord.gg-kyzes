-- ══════════════════════════════════════
-- TAB SWITCHING
-- ══════════════════════════════════════
local allTabs = {T1,T2,T3,T4,T5}

SwitchTab = function(idx)
    for i, t in ipairs(allTabs) do t.Visible=(i==idx) end
    for i, b in ipairs(NavBtns) do
        if i==idx then
            b.TextColor3=C.WHT
            TW(b,0.12,{BackgroundTransparency=0,BackgroundColor3=C.BG3})
        else
            b.TextColor3=C.MUT
            TW(b,0.12,{BackgroundTransparency=1})
        end
    end
    TW(NavInd,0.2,{Position=S(0,61+(idx-1)*46+5)})
end
SwitchTab(1)

-- ══════════════════════════════════════
-- TOGGLE HELPER
-- ══════════════════════════════════════
local function SetTog(btn, on, col)
    btn.Text = on and "ON" or "OFF"
    btn.TextColor3 = on and col or C.MUT
    local r,g,b = col.R*255*0.15, col.G*255*0.15, col.B*255*0.15
    TW(btn,0.15,{BackgroundColor3 = on and Color3.fromRGB(r,g,b) or C.BG3})
end

-- AFK
local function DoAFK(...)
    local on = ToggleAFK()
    for _, t in ipairs({...}) do SetTog(t,on,C.GRN) end
    AddNotif("Anti-AFK "..(on and "enabled" or "disabled"),C.GRN2,"🛡")
end
DashAFK.MouseButton1Click:Connect(function() DoAFK(DashAFK,U4AFK) end)
U4AFK.MouseButton1Click:Connect(function() DoAFK(U4AFK,DashAFK) end)

-- FPS
local function DoFPS(...)
    local ok, on = pcall(ToggleFPS)
    if not ok then
        for _, t in ipairs({...}) do t.Text="N/A" t.TextColor3=C.RED end
        AddNotif("setfpscap not supported on this executor",C.RED,"❌")
        return
    end
    for _, t in ipairs({...}) do SetTog(t,on,C.CYN) end
    AddNotif("FPS "..(on and "uncapped" or "locked to 60"),Color3.fromRGB(14,55,78),"⚡")
end
DashFPS.MouseButton1Click:Connect(function() DoFPS(DashFPS,U4FPS) end)
U4FPS.MouseButton1Click:Connect(function() DoFPS(U4FPS,DashFPS) end)

-- Quiet
U4NOTI.MouseButton1Click:Connect(function()
    quietMode = not quietMode
    SetTog(U4NOTI,quietMode,C.YEL)
end)

-- Copy HWID
U4Cp.MouseButton1Click:Connect(function()
    pcall(function() setclipboard(HWID) end)
    U4Cp.Text="✅  Copied!"
    task.delay(2,function() U4Cp.Text="📋  Copy HWID to Clipboard" end)
    AddNotif("HWID copied to clipboard",C.BLU3,"📋")
end)

-- Decompile
local isDecomp = false
D2Btn.MouseButton1Click:Connect(function()
    if isDecomp then return end
    isDecomp=true
    D2Btn.Text="⏳  Decompiling..."
    TW(D2Btn,0.1,{BackgroundColor3=C.BLU3})
    D2Stat.TextColor3=C.YEL D2Stat.Text="Starting..."
    D2PF.Size=S(0,4)
    TS:Create(D2PF,TweenInfo.new(18,Enum.EasingStyle.Linear),{Size=P(0.88,0,1,0)}):Play()
    task.spawn(function()
        AddLog("Started: "..gameName, C.BLU)
        local ok,folder = RunDecompiler(function(msg,prog)
            D2Stat.Text=msg
            pcall(AddLog,msg)
            if prog and prog>0 then
                TS:Create(D2PF,TweenInfo.new(0.4),{Size=P(prog,0,1,0)}):Play()
            end
        end)
        TS:Create(D2PF,TweenInfo.new(0.5),{Size=P(1,0,1,0)}):Play()
        task.wait(0.5)
        if ok then
            D2Btn.Text="✅  Done!"
            TW(D2Btn,0.1,{BackgroundColor3=Color3.fromRGB(22,110,55)})
            D2Stat.TextColor3=C.GRN D2Stat.Text="Saved! Check your executor workspace folder."
            AddNotif("Decompile complete: "..gameName:sub(1,26),C.GRN2,"✅")
            task.delay(6,function()
                D2Btn.Text="⚙  Decompile  "..gameName:sub(1,30)
                TW(D2Btn,0.15,{BackgroundColor3=C.BLU})
            end)
        else
            D2Btn.Text="❌  Failed — Retry?"
            TW(D2Btn,0.1,{BackgroundColor3=Color3.fromRGB(110,25,25)})
            D2Stat.TextColor3=C.RED
            AddNotif("Decompile failed — check log",C.RED,"❌")
            task.delay(5,function()
                D2Btn.Text="⚙  Decompile  "..gameName:sub(1,30)
                TW(D2Btn,0.15,{BackgroundColor3=C.BLU})
            end)
        end
        isDecomp=false
    end)
end)

-- Map
local isMap = false
M3Btn.MouseButton1Click:Connect(function()
    if isMap then return end
    isMap=true M3Btn.Text="⏳  Loading..."
    TW(M3Btn,0.1,{BackgroundColor3=C.GRN2}) M3PF.Size=S(0,4)
    AddNotif("Map load started",C.GRN2,"🗺")
    task.spawn(function()
        LoadFullMap(function(msg,prog)
            M3St.Text=msg M3St.TextColor3=C.YEL
            if prog then TS:Create(M3PF,TweenInfo.new(0.3),{Size=P(prog,0,1,0)}):Play() end
        end)
        M3Btn.Text="✅  Map Loaded!"
        TW(M3Btn,0.1,{BackgroundColor3=Color3.fromRGB(22,110,55)})
        M3St.Text="Complete! Entire world loaded." M3St.TextColor3=C.GRN
        AddNotif("Map fully loaded!",C.GRN2,"✅")
        isMap=false
    end)
end)

