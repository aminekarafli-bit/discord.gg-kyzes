    ib.BackgroundTransparency = 0.5
    Label(ib, icon or "⚡", 13, C.WHT, P(1,0,1,0), S(0,0), nil, Enum.TextXAlignment.Center, 5)
    Label(card, msg, 10, C.WHT, S(176,28), S(36,4), nil, nil, 4)
    Label(card, os.date("%H:%M:%S"), 9, C.MUT, S(176,14), S(36,34), nil, nil, 4)
    task.wait()
    NS.CanvasPosition = Vector2.new(0, math.huge)
end

