-- ══════════════════════════════════════
-- UI HELPERS
-- ══════════════════════════════════════
local TS = TweenService
local function TW(o,t,p) TS:Create(o,TweenInfo.new(t,Enum.EasingStyle.Quart,Enum.EasingDirection.Out),p):Play() end
local function S(x,y)    return UDim2.new(0,x,0,y) end
local function P(a,b,c,d) return UDim2.new(a,b,c,d) end

local function N(cls, props, parent)
    local o = Instance.new(cls)
    for k,v in pairs(props) do pcall(function() o[k] = v end) end
    if parent then o.Parent = parent end
    return o
end
local function Rnd(p,r) N("UICorner",{CornerRadius=UDim.new(0,r or 8)},p) end
local function Bdr(p,col,th) return N("UIStroke",{Color=col or C.BG4,Thickness=th or 1},p) end
local function Pad(p,l,r,t,b)
    N("UIPadding",{PaddingLeft=UDim.new(0,l),PaddingRight=UDim.new(0,r),
        PaddingTop=UDim.new(0,t),PaddingBottom=UDim.new(0,b)},p)
end

local function Frame(parent, bg, size, pos, z, radius, border)
    local f = N("Frame",{BackgroundColor3=bg or C.BG2,BorderSizePixel=0,
        Size=size or S(100,40),Position=pos or S(0,0),ZIndex=z or 1}, parent)
    if radius then Rnd(f, radius) end
    if border  then Bdr(f, border) end
    return f
end

local function Label(parent, text, tsize, col, size, pos, font, align, z)
    return N("TextLabel",{Parent=parent,BackgroundTransparency=1,
        Text=text or "", TextSize=tsize or 13, TextColor3=col or C.WHT,
        Size=size or P(1,0,0,20), Position=pos or S(0,0),
        Font=font or Enum.Font.Gotham,
        TextXAlignment=align or Enum.TextXAlignment.Left,
        TextWrapped=true, ZIndex=z or 1})
end

local function Button(parent, text, bg, hover, size, pos, tsize, z, radius)
    local bgN = bg or C.BLU
    local bgH = hover or C.BLU2
    local b = N("TextButton",{Parent=parent,BackgroundColor3=bgN,BorderSizePixel=0,
        TextColor3=C.WHT,Font=Enum.Font.GothamBold,TextSize=tsize or 13,
        Text=text or "",Size=size or S(200,36),Position=pos or S(0,0),
        AutoButtonColor=false,ZIndex=z or 1})
    Rnd(b, radius or 8)
    b.MouseEnter:Connect(function() TW(b,0.12,{BackgroundColor3=bgH}) end)
    b.MouseLeave:Connect(function() TW(b,0.12,{BackgroundColor3=bgN}) end)
    return b
end

local function Rainbow(obj, prop, spd, sat, val, hOff)
    task.spawn(function()
        local h = hOff or math.random(0,360)
        while obj and obj.Parent do
            h = (h + (spd or 0.5)) % 360
            obj[prop] = Color3.fromHSV(h/360, sat or 0.8, val or 1)
            task.wait(0.035)
        end
    end)
end

-- Gradient username label
local function GradLabel(parent, text, tsize, pos, fsize, z)
    local holder = N("Frame",{Parent=parent,BackgroundTransparency=1,
        Size=fsize or S(200,26),Position=pos or S(0,0),ZIndex=z or 2})
    local lbl = N("TextLabel",{Parent=holder,BackgroundTransparency=1,
        Text=text,TextSize=tsize or 18,Font=Enum.Font.GothamBold,
        TextColor3=C.WHT,Size=P(1,0,1,0),
        TextXAlignment=Enum.TextXAlignment.Left,ZIndex=z or 2})
    N("UIGradient",{
        Color=ColorSequence.new({
            ColorSequenceKeypoint.new(0,   Color3.fromRGB(110,170,255)),
            ColorSequenceKeypoint.new(0.4, Color3.fromRGB(190,130,255)),
            ColorSequenceKeypoint.new(0.75,Color3.fromRGB(90,210,255)),
            ColorSequenceKeypoint.new(1,   Color3.fromRGB(55,245,185)),
        }),
        Rotation=18,
    },lbl)
    return lbl
end

