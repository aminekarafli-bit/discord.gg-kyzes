-- README builder
local function BuildReadMe()
    local NL = "\n"
    return "-- Decompiled with Zyler Hub v"..VERSION.." | "..DISCORD..NL
        .."-- Game: "..gameName.." | PlaceId: "..placeId..NL
        .."-- Date: "..os.date("%Y-%m-%d %H:%M:%S")..NL
end

-- Branding replacement (no \n in pattern strings — use string.char(10))
local function ReplaceBranding(folderPath)
    local NL = string.char(10)
    local bad = {
        "UniversalSynSaveInstance",
        "discord.gg/wx4ThpAsmw",
        "Saved by UniversalSynSaveInstance",
        "discord.gg/svHEHMPJBB",
        "Join to Copy Games",
    }
    local good = "-- Decompiled with Zyler Hub v"..VERSION.." | "..DISCORD

    local function hasBad(s)
        for _,p in ipairs(bad) do if s:find(p,1,true) then return true end end
        return false
    end

    local function procFile(path)
        local ok, content = pcall(readfile, path)
        if not ok or not content or not hasBad(content) then return end
        local out, replaced = {}, false
        -- Split on NL character safely
        local function eachLine(str)
            local lines = {}
            local pos = 1
            while pos <= #str do
                local nl = str:find(NL, pos, true)
                if nl then
                    lines[#lines+1] = str:sub(pos, nl-1)
                    pos = nl + 1
                else
                    lines[#lines+1] = str:sub(pos)
                    break
                end
            end
            return lines
        end
        for _, line in ipairs(eachLine(content)) do
            if hasBad(line) then
                if not replaced then out[#out+1] = good replaced = true end
            else
                out[#out+1] = line
            end
        end
        pcall(writefile, path, table.concat(out, NL))
    end

    local function scan(path)
        local ok1, files = pcall(listfiles, path)
        if ok1 and files then
            for _, f in ipairs(files) do
                if f:match("%.lua$") or f:match("%.luau$") or f:match("%.txt$") then
                    pcall(procFile, f)
                end
            end
        end
        local ok2, dirs = pcall(listfolders, path)
        if ok2 and dirs then for _, d in ipairs(dirs) do scan(d) end end
    end
    pcall(scan, folderPath)
end

-- Anti-AFK
local afkConn, afkOn = nil, false
local function ToggleAFK()
    afkOn = not afkOn
    if afkOn then
        afkConn = RunService.Heartbeat:Connect(function()
            pcall(function() LocalPlayer:Move(Vector3.zero, false) end)
        end)
    else
        if afkConn then afkConn:Disconnect() end
    end
    return afkOn
end

-- FPS
local fpsOn = false
local function ToggleFPS()
    fpsOn = not fpsOn
    pcall(function() if fpsOn then setfpscap(0) else setfpscap(60) end end)
    return fpsOn
end

-- Open Discord
local function OpenDiscord()
    local url = "https://"..DISCORD
    local ok = false
    pcall(function() if syn and syn.open_url then syn.open_url(url) ok=true end end)
    pcall(function() if not ok and openurl then openurl(url) ok=true end end)
    pcall(function() setclipboard(url) end)
end

-- Map Loader
local function LoadFullMap(onStep)
    onStep("Disabling StreamingEnabled...", 0)
    pcall(function() workspace.StreamingEnabled = false end)
    task.wait(0.3)
    local pts = {
        Vector3.new(0,0,0),      Vector3.new(500,0,500),   Vector3.new(-500,0,500),
        Vector3.new(500,0,-500), Vector3.new(-500,0,-500),  Vector3.new(1000,0,0),
        Vector3.new(-1000,0,0),  Vector3.new(0,0,1000),    Vector3.new(0,0,-1000),
        Vector3.new(2000,0,2000),Vector3.new(-2000,0,-2000),
        Vector3.new(3000,0,0),   Vector3.new(0,0,3000),
    }
    for i, pt in ipairs(pts) do
        pcall(function() LocalPlayer:RequestStreamAroundAsync(pt, 2) end)
        onStep("Requesting chunks "..i.."/"..#pts, i/(#pts+1))
        task.wait(0.2)
    end
    onStep("Camera sweep...", 0.9)
    local cam = workspace.CurrentCamera
    local oldCF, oldType = cam.CFrame, cam.CameraType
    cam.CameraType = Enum.CameraType.Scriptable
    for _, pt in ipairs(pts) do
        cam.CFrame = CFrame.new(pt + Vector3.new(0,400,0), pt)
        task.wait(0.07)
    end
    cam.CameraType = oldType
    cam.CFrame = oldCF
    stats.mapLoads = stats.mapLoads + 1
    onStep("Map fully loaded!", 1)
end

-- Decompiler
local function RunDecompiler(onStep)
    local snap = {}
    for _, v in ipairs(CoreGui:GetChildren()) do snap[v] = true end
    for _, v in ipairs(LocalPlayer.PlayerGui:GetChildren()) do snap[v] = true end
    local watcher = RunService.Heartbeat:Connect(function()
        for _, v in ipairs(CoreGui:GetChildren()) do
            if not snap[v] and v.Name ~= GUI_NAME then pcall(v.Destroy, v) end
        end
        for _, v in ipairs(LocalPlayer.PlayerGui:GetChildren()) do
            if not snap[v] then pcall(v.Destroy, v) end
        end
    end)

    onStep("Fetching save engine...", 0.05)
    local safeName = gameName:gsub("[^%w%-_]","_"):sub(1,40)

    local ok, err = pcall(function()
        local ssi = loadstring(game:HttpGet(
            "https://raw.githubusercontent.com/verysigmapro/UniversalSynSaveInstance-With-Save-Terrain/refs/heads/main/saveinstance.luau",
            true))()
        onStep("Saving "..gameName.." ...", 0.12)
        ssi({
            mode="full", ShowStatus=false, ReadMe=false,
            Decompile=true, DecompileTimeout=15,
            scriptcache=true, SaveBytecode=false,
            NilInstances=false, IgnoreDefaultProperties=true,
            IgnoreNotArchivable=true, IgnoreDefaultPlayerScripts=true,
            RemovePlayerCharacters=true, IsolatePlayers=false,
            IsolateLocalPlayer=false, IsolateLocalPlayerCharacter=false,
            AntiIdle=false, FilePath=safeName,
        })
    end)

    watcher:Disconnect()

    if not ok then
        stats.errors = stats.errors + 1
        onStep("Error: "..tostring(err):sub(1,80), -1)
        return false
    end

    pcall(function() writefile(safeName.."/ZylerHub_README.lua", BuildReadMe()) end)
    onStep("Replacing branding...", 0.95)
    pcall(function() ReplaceBranding(safeName) end)
    stats.decomps = stats.decomps + 1
    onStep("Done! Saved to workspace/"..safeName, 1)
    return true, safeName
end

-- KeyAuth
local function InitSession()
    local url = ("https://keyauth.win/api/1.2/?type=init&ver=%s&name=%s&ownerid=%s")
        :format(VERSION, HttpService:UrlEncode(APP_NAME), OWNER_ID)
    local ok, res = pcall(function() return game:HttpGet(url) end)
    if not ok then return nil end
    local dok, d = pcall(HttpService.JSONDecode, HttpService, res)
    return dok and d.success and d.sessionid or nil
end

local function ValidateKey(key)
    local sid = InitSession()
    if not sid then return false, "Connection failed" end
    local url = ("https://keyauth.win/api/1.2/?type=license&key=%s&sessionid=%s&name=%s&ownerid=%s&hwid=%s")
        :format(HttpService:UrlEncode(key), sid, HttpService:UrlEncode(APP_NAME), OWNER_ID, HttpService:UrlEncode(HWID))
    local ok, res = pcall(function() return game:HttpGet(url) end)
    if not ok then return false, "Connection error" end
    local dok, d = pcall(HttpService.JSONDecode, HttpService, res)
    if not dok then return false, "Parse error" end
    return d.success == true, tostring(d.message)
end

