-- Live timer
task.spawn(function()
    while T1.Parent do
        local e = os.time()-sessionStart
        local t = string.format("%02d:%02d",math.floor(e/60),e%60)
        pcall(function() StatLbl.Text="● Active  ·  "..t end)
        pcall(function() if UptLbl then UptLbl.Text=t end end)
        for k,lbl in pairs(circVals) do
            pcall(function() lbl.Text=tostring(stats[k] or 0) end)
        end
        task.wait(1)
    end
end)

