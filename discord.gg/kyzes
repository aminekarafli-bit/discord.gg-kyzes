end)

-- ══════════════════════════════════════
-- KEY FLOW
-- ══════════════════════════════════════
local function ShowMain()
    TW(KF,0.3,{Position=P(0.5,-230,1.6,0)})
    task.wait(0.32)
    KF.Visible=false
    MW.Position=P(0.5,-450,1.7,0) MW.Visible=true
    TW(MW,0.45,{Position=P(0.5,-450,0.5,-255)})
    task.wait(0.55)
    AddNotif("Logged in as "..username, C.BLU3,"⚡")
    AddNotif("Target: "..gameName:sub(1,30), C.BLU3,"🎮")
    AddNotif("discord.gg/kyzes — get free keys!", C.DISC,"💬")
end

local checking = false
task.spawn(function()
    local d = 0
    while KBtn.Parent do
        task.wait(0.32)
        if checking then d=(d%3)+1 KBtn.Text="Verifying"..string.rep(".",d) end
    end
end)

if isfile and isfile(KEY_FILE) then
    local saved = readfile(KEY_FILE)
    if saved and #saved > 5 then
        KMsg.TextColor3=C.GRN KMsg.Text="⏳  Checking saved key..."
        task.spawn(function()
            local ok, msg = ValidateKey(saved)
            if ok then ShowMain()
            else
                pcall(function() writefile(KEY_FILE,"") end)
                KMsg.TextColor3=C.RED KMsg.Text="❌  Saved key expired — re-enter."
            end
        end)
    end
end

KBtn.MouseButton1Click:Connect(function()
    local key = KBox.Text
    if key=="" then KMsg.TextColor3=C.YEL KMsg.Text="⚠  Please enter your key." return end
    checking=true TW(KBtn,0.1,{BackgroundColor3=C.BLU3}) KMsg.Text=""
    task.spawn(function()
        local ok, msg = ValidateKey(key)
        checking=false KBtn.Text="🔓  Verify Key"
        if ok then
            pcall(function() writefile(KEY_FILE,key) end)
            KMsg.TextColor3=C.GRN KMsg.Text="✅  Key verified!"
            TW(KBtn,0.1,{BackgroundColor3=Color3.fromRGB(26,108,52)})
            task.wait(0.7) ShowMain()
        else
            TW(KBtn,0.1,{BackgroundColor3=C.BLU})
            KMsg.TextColor3=C.RED KMsg.Text="❌  "..(msg or "Invalid key")
        end
    end)
end)
